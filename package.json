{
  "name": "Focus",
  "version": "0.0.1",
  "description": "What a Blog make you Focused!",
  "author": "Innei",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon --exec babel-node server/index.js --extensions \".js,.ts,.tsx\" --watch server",
    "dev:no-web": "cross-env NODE_ENV=development VUE_ENV=no-web nodemon --exec babel-node --extensions \".js,.ts,.tsx\" server/index.js --watch server",
    "build": "nuxt build && babel server -d server/build",
    "server": "yarn dev:no-web",
    "build-server": "babel server -d server/build --extensions \".js,.ts,.tsx\"",
    "start": "pm2 start ecosystem.js --env production",
    "analyze": "nuxt build --analyze",
    "build:modern": "nuxt build --modern=server",
    "start:modern": "nuxt start --modern=server",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore ."
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "apicache": "^1.5.2",
    "bcrypt": "^3.0.7",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "element-ui": "^2.13.0",
    "express": "^5.0.0-alpha.7",
    "express-swagger-generator": "^1.1.15",
    "express-useragent": "^1.0.13",
    "express-ws": "^4.0.0",
    "http-assert": "^1.4.1",
    "inflection": "^1.12.0",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "markdown-it": "^10.0.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-prism": "^2.0.3",
    "moment": "^2.24.0",
    "mongo-sanitize": "^1.0.1",
    "mongoose": "^5.7.13",
    "mongoose-unique-validator": "^2.0.3",
    "morgan": "^1.9.1",
    "nanoid": "^2.1.11",
    "nodemailer": "^6.4.2",
    "nuxt": "^2.11.0",
    "prismjs": "^1.17.1",
    "qs": "^6.9.1",
    "redis": "^2.8.0",
    "rss": "^1.2.2",
    "sitemap": "^5.1.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-express": "^4.1.2",
    "vue-awesome-swiper": "^3.1.3",
    "vue-progressive-image": "^3.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.7.4",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@nuxtjs/eslint-config": "^2.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "node-sass": "^4.13.0",
    "nodemon": "^2.0.2",
    "pm2": "^4.2.3",
    "prettier": "^1.16.4",
    "sass-loader": "^8.0.0"
  }
}
