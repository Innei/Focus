// this babelrc only for server node-babel

{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-proposal-class-properties",
    [
      "module-resolver",
      {
        "root": ["./server"],
        "alias": {
          "~": "./server"
        }
      }
    ]
  ]
}
